<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:TestThemes"
	xmlns:reskeys="clr-namespace:MLib.Themes;assembly=MLib">

	<!--  Pop-Up color must be themed this way since Generic demo app does not use MLib definitions  -->
	<SolidColorBrush x:Key="PopUpBackground" Color="{DynamicResource {x:Static reskeys:ResourceKeys.ControlPopupBackgroundKey}}" />

	<SolidColorBrush x:Key="PopUpBorder" Color="{DynamicResource {x:Static reskeys:ResourceKeys.ControlAccentColorKey}}" />

</ResourceDictionary>